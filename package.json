{
  "name": "helloworld",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --fix -c .eslintrc.json --ext=ts src/app -f html -o ./eslint/eslintResult.html",
    "e2e": "ng e2e",
    "eslint": "eslint --fix -c ts.eslintrc --ext=ts src/app -f html -o ./eslint/eslintResult.html",
    "format": "prettier --write src/app/**/*",
    "build-deploy-gh-pages": "ng build --prod --base-href 'https://bobbysy.github.io/angular-cesium-sample/' && npx ngh --dir dist/angular-cesium-sample"
  },
  "jest": {
    "name": "helloworld",
    "preset": "jest-preset-angular",
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformer": [
          "<rootDir>/node_modules/jest-preset-angular/InlineHtmlStripStylesTransformer"
        ]
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.4",
    "@angular/cdk": "^8.1.4",
    "@angular/cdk-experimental": "^8.1.4",
    "@angular/common": "^8.2.4",
    "@angular/compiler": "^8.2.4",
    "@angular/core": "^8.2.4",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.4",
    "@angular/http": "^7.2.16",
    "@angular/material": "^8.1.4",
    "@angular/material-experimental": "^8.1.4",
    "@angular/material-moment-adapter": "^8.1.4",
    "@angular/platform-browser": "^8.2.4",
    "@angular/platform-browser-dynamic": "^8.2.4",
    "@angular/router": "^8.2.4",
    "angular-cesium": "0.0.64",
    "angular-cli-ghpages": "^0.6.2",
    "bootstrap": "^4.4.1",
    "cesium": "^1.66.0",
    "core-js": "^3.6.4",
    "hammerjs": "^2.0.8",
    "jest-preset-angular": "^8.0.0",
    "npm": "^6.13.7",
    "rxjs": "^6.5.4",
    "tslib": "^1.11.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular/cli": "~8.3.24",
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/compiler-cli": "^8.2.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@types/jest": "^25.1.3",
    "@angular/language-service": "^9.0.2",
    "@typescript-eslint/parser": "^2.20.0",
    "@types/node": "~13.7.7",
    "babel-eslint": "^10.1.0",
    "codelyzer": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.7",
    "prettier": "1.19.1",
    "protractor": "~5.4.3",
    "ts-node": "~8.6.2",
    "typescript": "~3.5.3"
  },
  "author": "sy",
  "license": "ISC"
}
